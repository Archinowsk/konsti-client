    "outdated-browser-rework": "2.9.0",
    "react": "16.10.2",
    "react-beautiful-dnd": "11.0.5",
    "react-dom": "16.10.2",
    "react-i18next": "10.13.1",
    "react-redux": "7.1.1",
    "react-router-dom": "5.1.2",
    "redux": "4.0.4",
    "redux-form": "8.2.6",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.3",
    "request": "2.88.0",
    "request-promise-native": "1.0.7"
