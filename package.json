{
  "name": "konsti-client",
  "version": "0.0.1",
  "private": true,
  "main": "server.js",
  "description": "Konsti signup tool client",
  "author": {
    "name": "Arttu Hanska"
  },
  "scripts": {
    "analyze": "webpack-cli --profile --json > stats.json && webpack-bundle-analyzer stats.json -m assets -r report.html",
    "build": "webpack-cli",
    "build-staging": "webpack-cli",
    "eslint": "eslint src",
    "flow": "flow check",
    "flow-typed-install": "flow-typed install --skip --overwrite --ignoreDeps peer bundle",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{js,json,scss}\"",
    "start": "webpack-dev-server",
    "stylelint": "stylelint \"src/**/*.scss\" --fix"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0-beta.54",
    "axios": "0.18.0",
    "i18next": "11.4.0",
    "i18next-browser-languagedetector": "2.2.2",
    "i18next-xhr-backend": "1.5.1",
    "lodash": "4.17.10",
    "moment": "2.22.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-i18next": "7.9.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "redux-form": "7.4.2",
    "redux-thunk": "2.3.0",
    "request": "2.87.0",
    "request-promise-native": "1.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.54",
    "@babel/preset-env": "7.0.0-beta.54",
    "@babel/preset-flow": "7.0.0-beta.54",
    "@babel/preset-react": "7.0.0-beta.54",
    "autoprefixer": "9.0.0",
    "babel-eslint": "8.2.6",
    "babel-loader": "8.0.0-beta.4",
    "bundle-loader": "0.5.6",
    "clean-webpack-plugin": "0.1.19",
    "compression-webpack-plugin": "1.1.11",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "doiuse": "4.1.0",
    "dotenv-webpack": "1.5.7",
    "eslint": "5.2.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-standard": "12.0.0-alpha.0",
    "eslint-config-standard-react": "6.0.0",
    "eslint-plugin-compat": "2.5.1",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-flowtype-errors": "3.6.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.10.0",
    "eslint-plugin-standard": "3.1.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "flow-bin": "0.77.0",
    "flow-typed": "2.5.1",
    "html-webpack-plugin": "3.2.0",
    "html-webpack-template": "6.2.0",
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "moment-locales-webpack-plugin": "1.0.7",
    "node-sass": "4.9.2",
    "postcss": "7.0.1",
    "postcss-autoreset": "2.0.2",
    "postcss-flexbugs-fixes": "4.0.0",
    "postcss-loader": "2.1.6",
    "postcss-normalize": "6.0.0",
    "postcss-scss": "2.0.0",
    "prettier": "1.13.7",
    "react-hot-loader": "4.3.3",
    "sass-loader": "7.0.3",
    "style-loader": "0.21.0",
    "stylelint": "9.3.0",
    "stylelint-config-prettier": "3.3.0",
    "stylelint-config-sass-guidelines": "5.0.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-no-unsupported-browser-features": "3.0.1",
    "stylelint-scss": "3.2.0",
    "uglifyjs-webpack-plugin": "1.2.7",
    "webpack": "4.16.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.4",
    "webpack-merge": "4.1.3"
  },
  "lint-staged": {
    "src/**/*.json": [
      "prettier --parser json --write",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --syntax=scss --fix",
      "prettier --parser scss --write",
      "git add"
    ],
    "src/**/*.js": [
      "eslint --fix",
      "prettier --parser flow --write",
      "git add"
    ]
  }
}
