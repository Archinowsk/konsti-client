    "react": "16.12.0",
    "react-beautiful-dnd": "12.2.0",
    "react-dom": "16.12.0",
    "react-i18next": "11.3.1",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-form": "8.2.6",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.3",
    "request": "2.88.0",
    "request-promise-native": "1.0.8",
    "styled-components": "5.0.1"
