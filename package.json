    "eslint-plugin-unicorn": "12.1.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "4.2.0",
    "flow-bin": "0.109.0",
    "flow-coverage-report": "0.6.1",
