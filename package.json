    "rimraf": "3.0.0",
    "style-loader": "1.0.0",
    "stylelint": "11.1.1",
    "stylelint-a11y": "1.2.1",
    "stylelint-config-prettier": "6.0.0",
    "stylelint-config-standard": "19.0.0",
    "stylelint-no-unsupported-browser-features": "3.0.2",
