{
  "name": "konsti-client",
  "version": "0.0.1",
  "private": true,
  "main": "index.tsx",
  "description": "Konsti signup tool client",
  "author": {
    "name": "Arttu Hanska"
  },
  "engines": {
    "node": ">=12.x.x"
  },
  "scripts": {
    "build-dev": "webpack-cli",
    "build-staging": "webpack-cli",
    "build": "webpack-cli",
    "build-types": "tsc --emitDeclarationOnly",
    "bundle-analyze": "webpack-cli --profile --json > stats.json && webpack-bundle-analyzer stats.json build -m server",
    "clean": "rimraf build",
    "eslint-rules": "eslint --print-config .eslintrc.js",
    "eslint": "eslint . --ext .js,.ts,.tsx",
    "prettier:check": "prettier --write ./**/*.{js,json,md,ts,tsx,yml}",
    "prettier:write": "prettier --write ./**/*.{js,json,md,ts,tsx,yml}",
    "snapshot-update": "jest -u",
    "start": "webpack-dev-server",
    "stylelint": "stylelint ./**/*.{ts,tsx}",
    "test:coverage": "jest --coverage --colors",
    "test:watch": "jest --watch",
    "test": "jest",
    "lint": "npm run eslint && npm run stylelint && npm run prettier:check",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.26",
    "@fortawesome/free-solid-svg-icons": "5.12.0",
