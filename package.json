{
  "name": "konsti-client",
  "version": "0.0.1",
  "private": true,
  "main": "server.js",
  "description": "Konsti signup tool client",
  "author": {
    "name": "Arttu Hanska"
  },
  "scripts": {
    "build-dev": "webpack-cli",
    "build-staging": "webpack-cli",
    "build": "webpack-cli",
    "bundle-analyze": "webpack-cli --profile --json > stats.json && webpack-bundle-analyzer stats.json build -m server",
    "eslint-rules-table": "eslint-index .eslintrc.js --format table",
    "eslint-rules": "eslint-index .eslintrc.js",
    "eslint": "eslint src --fix",
    "flow-coverage-report": "flow-coverage-report -c 10 -i 'src/**/*.js'",
    "flow-typed-install": "rimraf flow-typed && flow-typed install --skip --ignoreDeps peer bundled",
    "flow": "flow check",
    "prettier": "prettier --write src/**/*.{js,json,css,md}",
    "snapshot-update": "jest -u",
    "start": "webpack-dev-server",
    "stylelint": "stylelint src/**/*.css --fix",
    "test:coverage": "jest --coverage --colors",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.25",
