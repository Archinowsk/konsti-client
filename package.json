{
  "name": "konsti-client",
  "version": "0.0.1",
  "private": true,
  "main": "server.js",
  "description": "Konsti signup tool client",
  "author": {
    "name": "Arttu Hanska"
  },
  "scripts": {
    "analyze": "webpack-cli --profile --json > stats.json && webpack-bundle-analyzer stats.json -m assets -r report.html",
    "build": "webpack-cli",
    "build-staging": "webpack-cli",
    "eslint": "eslint src",
    "list-rules": "eslint-index .eslintrc.js",
    "table-rules": "eslint-index .eslintrc.js --format table",
    "flow": "flow check",
    "flow-typed-install": "rimraf flow-typed && flow-typed install --skip --ignoreDeps peer bundle",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{js,json,scss}\"",
    "start": "webpack-dev-server",
    "stylelint": "stylelint \"src/**/*.scss\" --fix"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "axios": "0.18.0",
    "bowser": "2.0.0-beta.3",
    "browserslist": "4.3.3",
    "i18next": "12.0.0",
    "i18next-browser-languagedetector": "2.2.3",
    "i18next-xhr-backend": "1.5.1",
    "lodash": "4.17.11",
    "moment": "2.22.2",
    "outdated-browser-rework": "2.6.1",
    "react": "16.6.0",
    "react-beautiful-dnd": "9.0.2",
    "react-dom": "16.6.0",
    "react-i18next": "8.1.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-form": "7.4.2",
    "redux-thunk": "2.3.0",
    "request": "2.88.0",
    "request-promise-native": "1.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "autoprefixer": "9.3.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "brotli-webpack-plugin": "1.0.0",
    "bundle-loader": "0.5.6",
    "clean-webpack-plugin": "0.1.19",
    "compression-webpack-plugin": "2.0.0",
    "copy-webpack-plugin": "4.5.4",
    "css-loader": "1.0.0",
    "doiuse": "4.2.0",
    "dotenv-webpack": "1.5.7",
    "eslint": "5.7.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-config-standard": "12.0.0",
    "eslint-index": "1.5.0",
    "eslint-plugin-compat": "2.6.2",
    "eslint-plugin-eslint-comments": "3.0.1",
    "eslint-plugin-eslint-plugin": "1.4.1",
    "eslint-plugin-flowtype": "3.0.0",
    "eslint-plugin-flowtype-errors": "3.6.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-unicorn": "6.0.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "flow-bin": "0.84.0",
    "flow-typed": "2.5.1",
    "html-webpack-plugin": "3.2.0",
    "html-webpack-template": "6.2.0",
    "husky": "1.1.2",
    "lint-staged": "7.3.0",
    "moment-locales-webpack-plugin": "1.0.7",
    "node-sass": "4.9.4",
    "postcss": "7.0.5",
    "postcss-autoreset": "2.0.2",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.2.0",
    "postcss-scss": "2.0.0",
    "prepack-webpack-plugin": "1.1.2",
    "prettier": "1.14.3",
    "react-hot-loader": "4.3.11",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "stylelint": "9.6.0",
    "stylelint-config-prettier": "4.0.0",
    "stylelint-config-sass-guidelines": "5.2.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-no-unsupported-browser-features": "3.0.2",
    "stylelint-scss": "3.3.2",
    "uglifyjs-webpack-plugin": "2.0.1",
    "webpack": "4.23.1",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-merge": "4.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.json": [
      "prettier --parser json --write",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --syntax=scss --fix",
      "prettier --parser scss --write",
      "git add"
    ],
    "src/**/*.js": [
      "eslint --fix",
      "prettier --parser flow --write",
      "git add"
    ]
  }
}
