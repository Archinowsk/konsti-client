{
  "name": "konsti-client",
  "version": "0.0.1",
  "private": true,
  "main": "server.js",
  "description": "Konsti signup tool client",
  "author": {
    "name": "Arttu Hanska"
  },
  "scripts": {
    "analyze": "webpack-cli --profile --json > stats.json && webpack-bundle-analyzer stats.json -m assets -r report.html",
    "build": "webpack-cli",
    "build-staging": "webpack-cli",
    "build-dev": "webpack-cli",
    "eslint": "eslint src",
    "list-rules": "eslint-index .eslintrc.js",
    "table-rules": "eslint-index .eslintrc.js --format table",
    "flow": "flow check",
    "flow-typed-install": "rimraf flow-typed && flow-typed install --skip --ignoreDeps peer bundle",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{js,json,scss}\"",
    "start": "webpack-dev-server",
    "stylelint": "stylelint \"src/**/*.scss\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "snapshot-update": "jest -u"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.4",
    "@fortawesome/fontawesome-svg-core": "1.2.17",
    "@fortawesome/free-solid-svg-icons": "5.8.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "axios": "0.18.0",
    "babel-plugin-module-resolver": "3.2.0",
    "bowser": "2.4.0",
    "browserslist": "4.5.6",
    "i18next": "15.1.0",
    "i18next-browser-languagedetector": "3.0.1",
    "i18next-xhr-backend": "2.0.1",
    "moment": "2.24.0",
    "outdated-browser-rework": "2.8.0",
    "react": "16.8.6",
    "react-beautiful-dnd": "11.0.2",
    "react-dom": "16.8.6",
    "react-i18next": "10.9.0",
    "react-redux": "7.0.3",
    "react-router-dom": "5.0.0",
    "redux": "4.0.1",
    "redux-form": "8.2.0",
    "redux-thunk": "2.3.0",
    "request": "2.88.0",
    "request-promise-native": "1.0.7"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.4",
    "@hot-loader/react-dom": "16.8.6",
    "autoprefixer": "9.5.1",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.5",
    "brotli-webpack-plugin": "1.1.0",
    "bundle-loader": "0.5.6",
    "clean-webpack-plugin": "2.0.1",
    "compression-webpack-plugin": "2.0.0",
    "copy-webpack-plugin": "5.0.3",
    "css-loader": "2.1.1",
    "doiuse": "4.2.0",
    "dotenv-webpack": "1.7.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-config-standard": "12.0.0",
    "eslint-index": "1.5.0",
    "eslint-plugin-compat": "3.1.1",
    "eslint-plugin-eslint-comments": "3.1.1",
    "eslint-plugin-eslint-plugin": "2.0.1",
    "eslint-plugin-flowtype": "3.7.0",
    "eslint-plugin-flowtype-errors": "4.1.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jest": "22.5.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-node": "9.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "1.6.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-unicorn": "8.0.2",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "flow-bin": "0.98.0",
    "flow-typed": "2.5.1",
    "html-webpack-plugin": "3.2.0",
    "html-webpack-template": "6.2.0",
    "husky": "2.2.0",
    "jest": "24.8.0",
    "lint-staged": "8.1.6",
    "moment-locales-webpack-plugin": "1.0.7",
    "node-sass": "4.12.0",
    "postcss": "7.0.16",
    "postcss-autoreset": "2.0.2",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.6.0",
    "postcss-scss": "2.0.0",
    "prepack-webpack-plugin": "1.1.2",
    "prettier": "1.17.0",
    "react-hot-loader": "4.8.4",
    "redux-devtools-extension": "2.13.8",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "stylelint": "10.0.1",
    "stylelint-config-prettier": "5.1.0",
    "stylelint-config-sass-guidelines": "6.0.0",
    "stylelint-config-standard": "18.3.0",
    "stylelint-no-unsupported-browser-features": "3.0.2",
    "stylelint-scss": "3.6.1",
    "terser-webpack-plugin": "1.2.3",
    "uglifyjs-webpack-plugin": "2.1.2",
    "webpack": "4.30.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.3.1",
    "webpack-merge": "4.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.json": [
      "prettier --parser json --write",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --syntax=scss --fix",
      "prettier --parser scss --write",
      "git add"
    ],
    "src/**/*.js": [
      "eslint --fix",
      "prettier --parser flow --write",
      "git add"
    ]
  }
}
