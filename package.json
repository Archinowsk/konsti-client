{
  "name": "konsti-client",
  "version": "0.0.1",
  "private": true,
  "main": "server.js",
  "description": "Konsti signup tool client",
  "author": {
    "name": "Arttu Hanska"
  },
  "scripts": {
    "analyze": "webpack-cli --profile --json > stats.json && webpack-bundle-analyzer stats.json -m assets -r report.html",
    "build-dev": "webpack-cli",
    "build-staging": "webpack-cli",
    "build": "webpack-cli",
    "eslint": "eslint src --fix",
    "flow-coverage-report": "flow-coverage-report -c 10 -i 'src/**/*.js'",
    "flow-typed-install": "rimraf flow-typed && flow-typed install --skip --ignoreDeps peer bundle",
    "flow": "flow check",
    "list-rules": "eslint-index .eslintrc.js",
    "prettier": "prettier --write src/**/*.{js,json,css,md}",
    "snapshot-update": "jest -u",
    "start": "webpack-dev-server",
    "stylelint": "stylelint src/**/*.css --fix",
    "table-rules": "eslint-index .eslintrc.js --format table",
    "test:coverage": "jest --coverage --colors",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.4",
    "@fortawesome/fontawesome-svg-core": "1.2.19",
    "@fortawesome/free-solid-svg-icons": "5.9.0",
    "@fortawesome/react-fontawesome": "0.1.4",
    "axios": "0.19.0",
    "babel-plugin-module-resolver": "3.2.0",
    "bowser": "2.4.0",
    "browserslist": "4.6.2",
    "i18next": "17.0.3",
    "i18next-browser-languagedetector": "3.0.1",
    "i18next-xhr-backend": "3.0.0",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "outdated-browser-rework": "2.8.0",
    "react": "16.8.6",
    "react-beautiful-dnd": "11.0.3",
    "react-dom": "16.8.6",
    "react-i18next": "10.11.0",
    "react-redux": "7.0.3",
    "react-router-dom": "5.0.1",
    "redux": "4.0.1",
    "redux-form": "8.2.3",
    "redux-thunk": "2.3.0",
    "request": "2.88.0",
    "request-promise-native": "1.0.7"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.4",
    "@hot-loader/react-dom": "16.8.6",
    "@welldone-software/why-did-you-render": "3.1.0",
    "autoprefixer": "9.6.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-lodash": "3.3.4",
    "brotli-webpack-plugin": "1.1.0",
    "bundle-loader": "0.5.6",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.0.3",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "doiuse": "4.2.0",
    "dotenv-webpack": "1.7.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-standard": "12.0.0",
    "eslint-index": "1.5.0",
    "eslint-plugin-compat": "3.1.1",
    "eslint-plugin-eslint-comments": "3.1.1",
